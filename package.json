{
  "name": "@zendesk/react-measure-timing-hooks",
  "version": "0.0.0",
  "keywords": [
    "headless",
    "hook",
    "measure",
    "react",
    "time-to-interactive",
    "time-to-render",
    "timing",
    "tti",
    "zendesk"
  ],
  "bugs": {
    "url": "https://github.com/zendesk/react-measure-timing-hooks/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zendesk/react-measure-timing-hooks.git"
  },
  "sideEffects": false,
  "license": "Apache-2.0",
  "author": "Zendesk",
  "collaborators": [
    "Bazyli BrzoÃÅska <bbrzoska@zendesk.com>"
  ],
  "exports": {
    ".": {
      "import": "./esm/main.js",
      "require": "./cjs/main.js"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./cjs/*.js"
    },
    "./cjs": {
      "require": "./cjs/main.js"
    },
    "./cjs/*": {
      "require": "./cjs/*.js"
    },
    "./esm/*": {
      "import": "./esm/*.js"
    },
    "./src/*": {
      "default": "./src/*"
    },
    "./package.json": "./package.json"
  },
  "main": "cjs/main.js",
  "module": "esm/main.js",
  "source": "src/main.ts",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn copy:css",
    "build:cjs": "yarn rrun tsc --outDir cjs --module commonjs --target es2015",
    "build:esm": "rm -rf esm && yarn build:esm:ts && yarn build:esm:webpack",
    "build:esm:webpack": "yarn webpack build --mode development --entry ./src/main",
    "build:esm:ts": "yarn rrun tsc --outDir esm --module esnext --target es2015 --emitDeclarationOnly",
    "clean": "git clean -dfX --exclude=node_modules src && beemo typescript:sync-project-refs",
    "format": "yarn rrun prettier --write \"./{src,tests,.config}/**/!(*.d).{.js,jsx,ts,tsx,json,md}\"",
    "copy:css": "cp -f src/patternfly-globals.css cjs/patternfly-globals.css",
    "postinstallDev": "yarn prepare",
    "prepare": "rrun husky install .config/husky && beemo create-config",
    "release": "beemo run-script release",
    "test": "yarn test:format && yarn test:types && yarn test:lint && yarn test:code",
    "test:code": "rrun jest",
    "test:format": "yarn rrun prettier --check \"./{src,tests,.config}/**/!(*.d).{.js,jsx,ts,tsx,json,md}\"",
    "test:lint": "rrun eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test:types": "yarn rrun tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "main",
        "channel": false
      },
      {
        "name": "next",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "tagFormat": "${version}"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@mdx-js/react": "^2.3.0",
    "@niieani/scaffold": "^1.6.7",
    "@patternfly/patternfly": "^4.224.4",
    "@patternfly/react-charts": "^6.94.20",
    "@patternfly/react-core": "^4.276.9",
    "@patternfly/react-icons": "^4.93.6",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-backgrounds": "^7.0.2",
    "@storybook/addon-controls": "^7.0.2",
    "@storybook/addon-docs": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-measure": "^7.0.2",
    "@storybook/addon-outline": "^7.0.2",
    "@storybook/addon-toolbars": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/testing-library": "^0.1.0",
    "@types/babel__core": "^7.20.0",
    "@types/react": "^17.0.56",
    "@types/react-dom": "^17.0.19",
    "@types/react-test-renderer": "^17.0.2",
    "@types/webpack-sources": "^3.2.0",
    "css-loader": "^6.7.3",
    "re-resizable": "^6.9.9",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-test-renderer": "^17.0.0",
    "remark-gfm": "^3.0.1",
    "require-from-string": "^2.0.2",
    "storybook": "^7.0.2",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "webpack": "5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-sources": "^3.2.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "resolutions": {
    "@types/react": "17.0.48",
    "webpack@5.74.0": "patch:webpack@npm:5.74.0#.yarn/patches/webpack-npm-5.74.0-f5b838a00d.patch",
    "@storybook/node-logger": "7.0.2"
  },
  "packageManager": "yarn@3.2.2",
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "last 3 versions and > 1%",
    "not dead"
  ],
  "files": [
    "src",
    "cjs",
    "esm",
    "docs"
  ]
}
